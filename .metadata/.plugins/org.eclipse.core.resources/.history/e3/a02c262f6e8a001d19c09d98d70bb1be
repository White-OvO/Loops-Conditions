	num2 = in.nextDouble();
	num3 = in.nextDouble();
	
	// Solution 
	Scanner in = new Scanner(System.in);
	double num1, num2, num3;
	num1 = in.nextDouble();
	num2 = in.nextDouble();
	num3 = in.nextDouble();
	//solution
	double myDivision = (num1 / num2) / num3;
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
